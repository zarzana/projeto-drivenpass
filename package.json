{
  "name": "projeto-drivenpass",
  "version": "1.0.0",
  "description": "",
  "main": "src/server.ts",
  "scripts": {
    "start": "node ./dist/server.js",
    "build": "prisma migrate deploy && tsc -p tsconfig.build.json && tsc-alias",
    "test:load-envs": "dotenv -e .env.test",
    "test": "cross-env NODE_ENV=test jest --passWithNoTests --runInBand",
    "test:coverage": "cross-env NODE_ENV=test jest --passWithNoTests --runInBand --coverage",
    "test:migration:run": "npm run test:load-envs prisma migrate deploy",
    "test:migration:generate": "npm run test:load-envs prisma migrate dev",
    "dev:load-envs": "dotenv -e .env.development",
    "dev": "cross-env NODE_ENV=development nodemon ./src/server.ts",
    "dev:migration:run": "npm run dev:load-envs prisma migrate deploy",
    "dev:migration:generate": "npm run dev:load-envs prisma migrate dev"
  },
  "keywords": [],
  "author": "Gabriel Zarzana Espinoza",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^5.4.2",
    "axios": "^1.5.1",
    "bcrypt": "^5.1.1",
    "cross-env": "^7.0.3",
    "cryptr": "^6.3.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "http-status": "^1.7.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "tsc-alias": "^1.8.8"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.2.0",
    "@types/bcrypt": "^5.0.0",
    "@types/cryptr": "^4.0.1",
    "@types/express": "^4.17.19",
    "@types/jest": "^29.5.5",
    "@types/jsonwebtoken": "^9.0.3",
    "@types/node": "^20.8.4",
    "@types/supertest": "^2.0.14",
    "dotenv-cli": "^7.3.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "prisma": "^5.4.2",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=18.18.0 <19.0.0"
  }
}
